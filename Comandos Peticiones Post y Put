curl -i -X POST -HContent-Type:application/json -HAccept:application/json http://localhost:8080/blueprints -d "{""author"":""Lucas"",""points"":[{""x"":"140",""y"":"140"},{""x"":"115",""y"":"115"}],""name"":""Coronado""}"


curl -i -X PUT -HContent-Type:application/json -HAccept:application/json http://localhost:8080/blueprints/Lucas/Coronado -d "{""author"":""Lucas"",""points"":[{""x"":"10",""y"":"10"},{""x"":"10",""y"":"10"}],""name"":""Coronado""}"


